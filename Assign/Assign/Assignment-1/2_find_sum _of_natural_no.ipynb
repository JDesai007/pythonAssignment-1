   "source": [
    "#Find the sum of the first N natural number\n",
    "\n",
    "no = int(input(\"Enter the Natural Number: \"))\n",
    "ans = 0\n",
    "\n",
    "for i in range(0, no+1):\n",
    "    ans = ans + i\n",
    "\n",
    "print(\"Sum of Number is: \",ans)"
